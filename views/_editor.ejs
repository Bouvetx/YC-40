<div class="form-group row">
  <label class="col-form-label col-sm-2" for="question">Question</label>
  <div class="col-sm-8">
    <input autofocus class="form-control" name="question" value="<%= model.question%>" />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="reponse">Reponse</label>
  <div class="col-sm-7">
    <input class="form-control" name="reponse" value="<%= model.reponse%>" />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="domain">Domain</label>
  <div class="col-sm-7">
    <select id="domain" name="domain">
	  <option value="http">http</option>
	  <option value="javascript">javascript</option>
	  <option value="python">python</option>
	  <option value="nginx">nginx</option>
	  <option value="linux">linux</option>
	  <option value="node">node</option>
	</select>
  </div>
</div>